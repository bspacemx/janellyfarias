        {
          "alias": false,
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".marker-wrap:before when (@showContactList)": {
              "content": "\"\"",
              "display": "inline-block",
              "vertical-align": "middle",
              "border": "3px solid @primaryColor",
              "width": "~\"calc(@{widthImg}rem - 6px)\"",
              "height": "~\"calc(@{widthImg}rem - 6px)\"",
              "box-sizing": "initial"
            },
            ".mbr-text": {
              "width": "auto"
            },
            ".mbr-list .items-list, .contacts-box .mbr-flex": {
              "flex-direction": "row"
            },
            ".marker-wrap:before, .contacts-box .cotacts-icon": {
              "margin-right": "15px"
            },
            ".mbr-list .items-list when (@showContactList)": {
              "align-items": "baseline",
              "&:not(:last-child)": {
                "padding-bottom": "10px"
              }
            },
            ".cotacts-icon .amp-iconfont when (@showIcons)": {
              "font-size": "~\"@{iconsSize}rem\"",
              "width": "~\"@{iconsSize}rem\"",
              "& when ((@iconsSize * 16) > 30)": {
                "@media (max-width: 767px)": {
                  "font-size": "30px",
                  "width": "30px"
                }
              }
            },
            ".social .iconfont-wrapper when (@showSocial)": {
              "line-height": "~\"calc(@{socialSize}rem + 4px)\"",
              "width": "~\"@{socialSize}rem\"",
              "height": "~\"@{socialSize}rem\"",
              "& when ((@socialSize * 16) > 30)": {
                "@media (max-width: 767px)": {
                  "line-height": "34px",
                  "width": "30px",
                  "height": "30px"
                }
              }
            },
            ".social .amp-iconfont when (@showSocial)": {
              "font-size": "~\"calc(@{socialSize}rem * 0.42)\"",
              "width": "~\"calc(@{socialSize}rem * 0.42)\"",
              "line-height": "~\"calc(@{socialSize}rem - 4px)\"",
              "& when ((@socialSize * 16) > 30)": {
                "@media (max-width: 767px)": {
                  "font-size": "~\"calc(30px * 0.42)\"",
                  "width": "~\"calc(30px * 0.42)\"",
                  "line-height": "26px"
                }
              }
            },
            "@media (max-width: 767px)": {
              ".mbr-jc-e": {
                "justify-content": "center"
              }
            }
          },
          "_name": "contacts3",
          "_customHTML": "<section class=\"contacts3\" group=\"Contacts\" plugins=\"amp-main\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n    <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n    <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n    <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n    <header>Title</header>\n    <input type=\"checkbox\" title=\"Title\" name=\"showTitle\">\n    <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n    <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n        <option value=\"1\">Left</option>\n        <option value=\"2\" selected=\"\">Center</option>\n        <option value=\"3\">Right</option>\n    </select> \n    <header>Contacts</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showContactTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showContactSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"List\" name=\"showContactList\">\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"listCount\" min=\"1\" max=\"9\" step=\"1\" value=\"1\" condition=\"showContactList\">\n            <input type=\"range\" inline=\"\" title=\"Width Marker\" name=\"widthImg\" min=\"0.8\" max=\"3\" step=\"0.1\" value=\"0.8\" condition=\"showContactList\">\n            <input type=\"checkbox\" title=\"Title List\" name=\"showContactListTitle\" condition=\"showContactList\">\n            <input type=\"checkbox\" title=\"Text List\" name=\"showContactListText\" condition=\"showContactList\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showContactText\">\n        <input type=\"checkbox\" name=\"showAddress\" title=\"Address\">\n        <input type=\"checkbox\" name=\"showPhone\" title=\"Phone\">\n        <input type=\"checkbox\" name=\"showEmail\" title=\"E-mail\">\n        <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\" checked=\"\" condition=\"showAddress || showPhone || showEmail\">\n        <input type=\"range\" inline=\"\" title=\"Size\" name=\"iconsSize\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"1.5\" condition=\"showIcons\">\n        <input type=\"checkbox\" title=\"Show Social\" name=\"showSocial\" checked=\"\">\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"socialCount\" min=\"1\" max=\"10\" value=\"3\" step=\"1\" condition=\"showSocial\">\n            <input type=\"range\" inline=\"\" title=\"Size\" name=\"socialSize\" min=\"1.5\" max=\"5\" step=\"0.1\" value=\"2.9\" condition=\"showSocial\">\n        <select title=\"Align\" name=\"contactsAlign\" condition=\"showContactTitle || showContactSubtitle || showContactList || showContactText || showAddress || showPhone || showEmail || showSocial\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background9.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected=\"\">\n        </fieldset>\n        <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#f4f5f7\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"title-wrap mbr-pb-4\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left': titleAlign==1,\n                         'align-center': titleAlign==2,\n                         'align-right': titleAlign==3, 'mbr-px-2': fullWidth}\">\n            <h1 class=\"mbr-section-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".title-wrap .mbr-section-title\">\n                Contacts</h1>\n            <h2 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".title-wrap .mbr-section-subtitle\">Lorem ipsum dolor sit amet, consectetur adipisicing elit.</h2>\n        </div>\n        <div class=\"contacts-block\" mbr-class=\"{'align-center': contactsAlign == 2,\n                                          'align-right': contactsAlign == 3}\" mbr-if=\"showContactTitle || showContactSubtitle || showContactList || showContactText || showAddress || showPhone || showEmail || showSocial\">\n            <h3 class=\"contacts-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" mbr-if=\"showContactTitle\" data-app-selector=\".contacts-title\"><i style=\"font-weight: normal;\">\"Todo lo que hagas,</i></h3>\n            <h4 class=\"contacts-subtitle mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" mbr-if=\"showContactSubtitle\" data-app-selector=\".contacts-subtitle\"><i style=\"font-weight: normal;\">hazlo con amor\"</i></h4>\n            <div class=\"mbr-list\" mbr-list=\"\" mbr-list-grow=\"listCount\" mbr-if=\"showContactList\" mbr-class=\"{'mbr-pt-4': showContactTitle || showContactSubtitle}\">\n                <div class=\"mbr-flex items-list\" mbr-class=\"{'mbr-jc-c': contactsAlign == 2, 'mbr-jc-e': contactsAlign == 3}\">\n                    <div class=\"marker-wrap\"></div>\n                    <div class=\"cotacts-row\" mbr-if=\"showContactListTitle || showContactListText\">\n                        <h4 class=\"cotacts-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" mbr-if=\"showContactListTitle\" data-app-selector=\".items-list .cotacts-title\" mbr-class=\"{'mbr-pb-1':showContactListText}\">\n                            First Title</h4>\n                        <p class=\"mbr-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showContactListText\" data-app-selector=\".items-list .mbr-text\">\n                           Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor.</p>\n                     </div>\n                </div> \n            </div>\n            <p class=\"mbr-text cotacts-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-7\" mbr-if=\"showContactText\" data-app-selector=\".cotacts-text\" mbr-class=\"{'mbr-pt-3':showContactTitle || showContactSubtitle || showContactList}\">\n               Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.</p>\n            <div class=\"contacts-box\" mbr-if=\"showAddress || showPhone || showEmail || showSocial\" mbr-class=\"{'mbr-pt-4':showContactTitle || showContactSubtitle || showContactList || showContactText}\">\n                <div class=\"address mbr-flex\" mbr-if=\"showAddress\" mbr-class=\"{'mbr-jc-c': contactsAlign == 2, 'mbr-jc-e': contactsAlign == 3}\">\n                    <div class=\"cotacts-icon\" mbr-if=\"showIcons\">\n                        <span mbr-icon=\"\" class=\"amp-iconfont fa-map-marker fa\"></span>\n                    </div>\n                    <p class=\"mbr-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" data-app-selector=\".contacts-box .mbr-text\"><b>ADDRESS:</b><br><span mbr-link-type=\"address\">10 Street, Los Angeles, CA, 93050</span></p>\n                </div>\n                <div class=\"phone mbr-flex\" mbr-if=\"showPhone\" mbr-class=\"{'mbr-pt-3': showAddress, 'mbr-jc-c': contactsAlign == 2, 'mbr-jc-e': contactsAlign == 3}\">\n                    <div class=\"cotacts-icon\" mbr-if=\"showIcons\">\n                        <span mbr-icon=\"\" class=\"amp-iconfont fa-volume-control-phone fa\"></span>\n                    </div>\n                    <p class=\"mbr-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" data-app-selector=\".contacts-box .mbr-text\"><b>PHONE:</b><br><span mbr-link-type=\"phone\">1555-234-45-67<br>1234-567-89-12</span></p>\n                </div>\n                <div class=\"email mbr-flex\" mbr-if=\"showEmail\" mbr-class=\"{'mbr-pt-3': showPhone || showAddress, 'mbr-jc-c': contactsAlign == 2, 'mbr-jc-e': contactsAlign == 3}\">\n                    <div class=\"cotacts-icon\" mbr-if=\"showIcons\">\n                        <span mbr-icon=\"\" class=\"amp-iconfont fa-envelope fa\"></span>\n                    </div>\n                    <p class=\"mbr-text mbr-fonts-style mbr-regular\" mbr-theme-style=\"display-4\" data-app-selector=\".contacts-box .mbr-text\"><b>EMAIL:</b><br><span mbr-link-type=\"email\">demomail@test.org<br>demomail@test.org</span></p>\n                </div>\n                <div class=\"icons-list social\" mbr-if=\"showSocial\" mbr-list=\"\" mbr-list-grow=\"socialCount\" mbr-link-type=\"socials\" mbr-class=\"{'mbr-pt-4': showPhone || showAddress || showEmail}\">\n                    <a href=\"https://www.facebook.com/janellyguerafarias/\" target=\"_blank\">\n                        <span class=\"iconfont-wrapper\">\n                            <span mbr-icon=\"\" class=\"amp-iconfont fa-facebook-f fa\"></span>\n                        </span>\n                    </a>\n                    <a href=\"https://www.instagram.com/janellyfarias/\" target=\"_blank\">\n                        <span class=\"iconfont-wrapper\">\n                            <span mbr-icon=\"\" class=\"amp-iconfont fa-instagram fa\"></span>\n                        </span>\n                    </a>\n                    <a href=\"https://twitter.com/janellyfarias\" target=\"_blank\">\n                        <span class=\"iconfont-wrapper\">\n                            <span mbr-icon=\"\" class=\"amp-iconfont fa-twitter fa\"></span>\n                        </span>\n                    </a>\n                    \n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
